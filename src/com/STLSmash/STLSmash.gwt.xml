<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="STLSmash">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
    <inherits name="org.gwtbootstrap3.extras.slider.Slider"/>
    <inherits name="org.gwtbootstrap3.extras.select.Select"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.STLSmash.client.CharacterForge'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/STLSmashService' class='com.STLSmash.server.STLSmashServiceImpl'/>

</module>